<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
     android:background="@color/colorPrimaryDark"
    tools:context="com.oztaking.www.myanimation.MainActivity">




    <Button
        android:layout_margin="15dp"
        android:layout_gravity="bottom|right"
        android:background="@mipmap/circle1"
        android:id="@+id/btn_circle1"
        android:layout_width="50dp"
        android:layout_height="50dp"/>

    <Button
        android:layout_margin="15dp"
        android:layout_gravity="bottom|right"
        android:background="@mipmap/circle2"
        android:id="@+id/btn_circle2"
        android:layout_width="50dp"
        android:layout_height="50dp"/>

    <Button
        android:layout_margin="15dp"
        android:layout_gravity="bottom|right"
        android:background="@mipmap/circle3"
        android:id="@+id/btn_circle3"
        android:layout_width="50dp"
        android:layout_height="50dp"/>

    <Button
        android:layout_margin="15dp"
        android:layout_gravity="bottom|right"
        android:background="@mipmap/circle4"
        android:id="@+id/btn_circle4"
        android:layout_width="50dp"
        android:layout_height="50dp"/>

    <Button
        android:layout_margin="15dp"
        android:layout_gravity="bottom|right"
        android:background="@mipmap/circle5"
        android:id="@+id/btn_circle5"
        android:layout_width="50dp"
        android:layout_height="50dp"/>

    <Button
        android:layout_margin="15dp"
        android:layout_gravity="bottom|right"
        android:background="@mipmap/menu"
        android:id="@+id/btn_menu"
        android:layout_width="50dp"
        android:layout_height="50dp"/>





</FrameLayout>
